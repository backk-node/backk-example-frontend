    "@types/crypto-js": "4.0.2",
    "@types/jest": "26.0.24",
    "@types/jsonwebtoken": "8.5.5",
    "@types/lodash": "4.14.176",
    "@types/node": "12.20.36",
    "@types/react": "17.0.33",
    "@types/react-dom": "17.0.10",
    "@types/uuid": "8.3.1",
    "backk-frontend-utils": "1.0.90",
    "classnames": "2.3.1",
    "cross-env": "7.0.3",
    "crypto-js": "4.1.1",
    "dayjs": "1.10.7",
    "husky": "7.0.4",
    "jsonwebtoken": "8.5.1",
    "lint-staged": "11.2.6",
    "lodash": "4.17.21",
    "prettier": "2.4.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-scripts": "4.0.3",
